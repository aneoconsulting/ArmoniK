{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "group:allNonMajor",
    ":semanticCommits"
  ],
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "ArmoniK"
      ],
      "groupName": "release packages Armonik"
    },
    {
      "matchPackagePatterns": [
        "*"
      ],
      "excludePackagePrefixes": [
        "ArmoniK"
      ],
      "groupName": "other dependencies"
    }
  ]
}
