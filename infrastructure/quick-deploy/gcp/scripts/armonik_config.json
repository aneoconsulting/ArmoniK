{
  "armonik": {
    "version": "0.33.1",
    "images": {
      "polling_agent": "dockerhubaneo/armonik_pollingagent",
      "worker": "dockerhubaneo/armonik_worker_dll"
    },
    "environment": {
      "ASPNETCORE_ENVIRONMENT": "Production",
      "ASPNETCORE_URLS": "http://+:8080",
      "Logging__LogLevel__Default": "Information",
      "Logging__LogLevel__Microsoft": "Warning",
      "Logging__LogLevel__System": "Warning",
      "GrpcClient__Endpoint": "http://armonik-control:5001",
      "Redis__EndpointUrl": "localhost:6379",
      "MongoDB__Host": "localhost",
      "MongoDB__Port": "27017",
      "MongoDB__DatabaseName": "ArmoniK",
      "MongoDB__DataRetention": "10.00:00:00",
      "MongoDB__TableStorage__PollingDelayMin": "00:00:01",
      "MongoDB__TableStorage__PollingDelayMax": "00:00:10",
      "MongoDB__AllowInsecureTls": "true",
      "ComputePlane__MessageBatchSize": "1",
      "ComputePlane__WorkerCheckRetries": "10",
      "ComputePlane__WorkerCheckDelay": "00:00:10",
      "ComputePlane__TaskProcessingGracePeriod": "00:02:00",
      "InitWorker__WorkerCheckRetries": "10",
      "InitWorker__WorkerCheckDelay": "00:00:10",
      "Pollster__GraceDelay": "00:00:02",
      "Pollster__MaxErrorAllowed": "5",
      "ControlPlane__MessageBatchSize": "1"
    },
    "health_check": {
      "enabled": true,
      "port": 8080,
      "interval": 30,
      "timeout": 10,
      "retries": 3
    },
    "restart_policy": {
      "max_restarts": 5,
      "backoff_base": 2.0,
      "backoff_max": 300.0,
      "failure_threshold": 10,
      "restart_delay": 5
    },
    "docker": {
      "network": "bridge",
      "restart_policy": "no",
      "log_driver": "json-file",
      "log_options": {
        "max-size": "10m",
        "max-file": "3"
      },
      "memory_limit": "2g",
      "cpu_limit": "1.0"
    },
    "ports": {
      "polling_agent": 8080,
      "worker": 8081,
      "health_check": 8090
    },
    "partitions": {
      "default": {
        "image": "dockerhubaneo/armonik_worker_dll",
        "tag": "0.33.1",
        "environment": {},
        "health_check_port": 8081
      },
      "pymonik": {
        "image": "dockerhubaneo/harmonic_snake",
        "tag": "python-3.10.12",
        "environment": {},
        "health_check_port": 8082
      },
      "stream": {
        "image": "dockerhubaneo/armonik_core_stream_test_worker",
        "tag": "latest",
        "environment": {},
        "health_check_port": 8083
      },
      "htcmock": {
        "image": "dockerhubaneo/armonik_core_htcmock_test_worker",
        "tag": "latest",
        "environment": {},
        "health_check_port": 8084
      },
      "bench": {
        "image": "dockerhubaneo/armonik_core_bench_test_worker",
        "tag": "latest",
        "environment": {},
        "health_check_port": 8085
      }
    }
  },
  "gcp": {
    "metadata_server": "http://metadata.google.internal/computeMetadata/v1",
    "shutdown_on_failure": true,
    "max_total_failures": 10,
    "health_check_grace_period": 60
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "C:/ArmoniK/logs/armonik_lifecycle.log",
    "max_size": "10MB",
    "backup_count": 5
  }
}
