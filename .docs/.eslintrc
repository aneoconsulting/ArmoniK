{
  "extends": "@nuxtjs/eslint-config-typescript",
  "rules": {
    "vue/multi-word-component-names": [
      0
    ],
    "vue/no-multiple-template-root": [
      0
    ],
    "vue/no-restricted-syntax": [
      "error",
      {
        "selector": "VElement[name='a']",
        "message": "Use NuxtLink instead."
      }
    ]
  }
}
